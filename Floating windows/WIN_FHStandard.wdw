      	Area.L = MyWindow..X
      	Area.R = Area.L + MyWindow..Width
      	Area.T = gnYWin
      	Area.B = Area.T + MyWindow..Height
      	
      	// Draws a focus rectangle to display the new position of the window
      	API("user32", "DrawFocusRect", nDC, &Area)
      	
      	// Frees the screen DC
      	API("user32", "ReleaseDC", 0, nDC)
      	
      END
