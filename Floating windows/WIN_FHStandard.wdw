      // If a move is in progress
      IF gbMove THEN	
      	
      	// Retrieves the screen DC
      	nDC is int = API("user32", "GetDC", 0)
      	
      	// Retrieves the mouse position
      	nY is int = MouseYPos
      	
      	// Calculates the move performed
      	nStartY is int = nY - gnMouseY
      	
